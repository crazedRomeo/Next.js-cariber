{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "speaker_name": {
      "type": "string"
    },
    "teaser": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "contents": {
      "type": "dynamiczone",
      "components": [
        "components.topic-component",
        "components.suitable-component",
        "components.episode-component",
        "components.special-ep-component",
        "components.speaker-detail-component"
      ]
    },
    "total_hours": {
      "type": "decimal",
      "default": 0.00
    },
    "total_lessons": {
      "type": "decimal",
      "default": 0.00
    },
    "order_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "order_link": {
      "type": "string"
    },
    "header": {
      "type": "text"
    },
    "teaser_url": {
      "type": "string"
    },
    "course_name": {
      "type": "string"
    },
    "asset_download": {
      "type": "string"
    },
    "thumbnail_image": {
      "type": "string"
    },
    "publish_date": {
      "type": "datetime"
    },
    "episodes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::episode.episode",
      "mappedBy": "course"
    },
    "instructor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::instructor.instructor",
      "inversedBy": "courses"
    },
    "description": {
      "type": "richtext"
    }
  }
}
